Host *
# Automatically load keys into ssh-agent
  AddKeysToAgent yes
# Don't error out if you don't know what the Apple Keychain is
  IgnoreUnknown UseKeychain
# Use Apple Keychain for ssh-key passphrases
  UseKeychain yes
# Automatically forward ssh-agent to destination
  # ForwardAgent yes
# Use this as the default key for all connections
  # IdentityFile ~/.ssh/id_rsa
  # IdentityFile ~/.ssh/id_ed25519

Host gitlab
  HostName gitlab.ops.canceriq.com
  AddKeysToAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519

Host do-site
  Port 22
  hostname 104.131.59.194
  IdentityFile ~/.ssh/id_do

Host do-nextcloud
  Port 22
  hostname 68.183.126.27
  IdentityFile ~/.ssh/id_do

Host clearpi
  Port 22
  hostname 192.168.1.76
  User pi

Host blackpi
  Port 22
  hostname 192.168.1.77
  User pi

Host github
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_github
